// 1 FEB 2023 => SKILLRACK DAILY CHALLENGE
#include <stdio.h>

int main() 
{
    char ch1,ch2,c;
    scanf("%c\n",&ch1);
    scanf("%c",&ch2);
    int count=0;
    while(ch1<=ch2)
    {
        if(ch1 != 'a' && ch1 != 'u' && ch1 != 'o' && ch1 != 'i' && ch1 != 'e' )
        {
            count = count+1;
        }
        ch1++;
        
    }
    printf("%d",count);
    return 0;
}
